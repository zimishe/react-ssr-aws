{
  "name": "server-side",
  "version": "1.0.0",
  "description": "serer-side-rendering-sandbox",
  "main": "index.js",
  "scripts": {
    "build": "concurrently --kill-others-on-fail npm:build:client npm:build:server",
    "start": "concurrently --kill-others-on-fail npm:bundler npm:build:server",
    "build:server": "NODE_OPTIONS=--max-old-space-size=4096 webpack --config server/webpack.config.js",
    "build:client": "NODE_OPTIONS=--max-old-space-size=4096 webpack --config client/webpack.config.js",
    "bundler": "node bundler client/webpack.config.js",
    "dev": "concurrently --kill-others \"npm run bundler\" \"npm run build:server\"",
    "production": "node server/dist/server.bundle.js"
  },
  "author": "yevhen",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "concurrently": "^5.0.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-watch": "^6.0.1",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "start-server-webpack-plugin": "^2.2.5",
    "style-loader": "^1.1.2",
    "terser-webpack-plugin": "2.1.3",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-stats-plugin": "^0.3.1"
  },
  "dependencies": {
    "axios": "^0.19.1",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.3",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
